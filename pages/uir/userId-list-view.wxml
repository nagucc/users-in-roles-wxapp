<template name="userId-list-view">
  <view wx:if="{{ids.length}}">
    <view class="weui-cells__title">UserId</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell" wx:for="{{ids}}" wx:key="*this">
        <view class="weui-cell__hd weui-cell__hd_in-select-after">
          <view class="weui-label">{{item.appId}}</view>
        </view>
        <view class="weui-cell__bd">{{item.userId}}</view>
      </view>
    </view>
  </view>
</template>